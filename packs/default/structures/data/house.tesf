id "house";

num road = 6;

num current = 0;
while(current < road) {
    block(current,0,0,"minecraft:stone");
    current = current + 1;
}

num startx = -4;

while(startx <= 4) {
    num y = 0;
    while(y <= 8) {
        block(current+startx,0,y - 4,"minecraft:stone");
        y = y + 1;
    }
    startx = startx + 1;
}