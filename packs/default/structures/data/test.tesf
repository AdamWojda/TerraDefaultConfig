id "test";

num y = 0;

while((check(0, y, 0) != "LAND") && (y > -50)) {
    y = y - 1;
}

num iter = 0;

while(iter < 10) {
    iter = iter + 1;
    if(check(0,y+iter,iter) == "LAND") {
        block(0, y+iter, iter, "minecraft:air");
    }
    if(check(0,y+iter,iter) == "AIR") {
        block(0, y+iter, iter, "minecraft:stone");
    }
    if(check(0,y+iter,iter) == "OCEAN") {
        block(0, y+iter, iter, "minecraft:green_wool");
    }
}

structure(0, y+iter, iter, "part2", "NONE", "CW_90", "CCW_90");
